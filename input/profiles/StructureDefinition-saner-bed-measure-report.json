{
  "resourceType": "StructureDefinition",
  "id": "saner-bed-measure-report",
  "url": "http://ainq.com/fhir/us/saner/StructureDefinition/saner-bed-measure-report",
  "version": "0.1.0",
  "name": "BedMeasureReport",
  "title": "Bed MeasureReport Profile",
  "status": "active",
  "description": "Defines constraints on the MeasureReport Resource for reporting summary data about Bed availability.",
  "fhirVersion": "4.0.1",
  "mapping": [
    {
      "identity": "w5",
      "uri": "http://hl7.org/fhir/fivews",
      "name": "FiveWs Pattern Mapping"
    },
    {
      "identity": "rim",
      "uri": "http://hl7.org/v3",
      "name": "RIM Mapping"
    }
  ],
  "kind": "resource",
  "abstract": false,
  "type": "MeasureReport",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/MeasureReport",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "MeasureReport.status",
        "path": "MeasureReport.status",
        "comment": "Typical use would be complete, but pending might be used in certain situations",
        "binding": {
          "strength": "required",
          "valueSet": "http://ainq.com/fhir/us/saner/ValueSet/OnlyCompleteOrPending"
        }
      },
      {
        "id": "MeasureReport.type",
        "path": "MeasureReport.type",
        "comment": "Type is always summary for reports",
        "patternCode": "summary"
      },
      {
        "id": "MeasureReport.measure",
        "path": "MeasureReport.measure",
        "comment": "The measure involved in report must be sent"
      },
      {
        "id": "MeasureReport.subject",
        "path": "MeasureReport.subject",
        "comment": "The subject involved in report must be sent",
        "min": 1,
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "http://hl7.org/fhir/StructureDefinition/Location"
            ]
          }
        ]
      },
      {
        "id": "MeasureReport.date",
        "path": "MeasureReport.date",
        "comment": "This is the date the summary report was generated, not the time when it was reporting about",
        "min": 1
      },
      {
        "id": "MeasureReport.reporter",
        "path": "MeasureReport.reporter",
        "comment": "The organization responsible for the report must be sent",
        "min": 1,
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "http://hl7.org/fhir/StructureDefinition/Organization"
            ]
          }
        ]
      },
      {
        "id": "MeasureReport.period",
        "path": "MeasureReport.period",
        "comment": "The period associated with the report must be sent."
      },
      {
        "id": "MeasureReport.group",
        "path": "MeasureReport.group",
        "slicing": {
          "discriminator": [
            {
              "type": "pattern",
              "path": "code"
            }
          ],
          "rules": "open",
          "description": "Slice based on the group.code pattern"
        },
        "comment": "At least one group must be present",
        "min": 1
      },
      {
        "id": "MeasureReport.group.population.code",
        "path": "MeasureReport.group.population.code",
        "comment": "Must include code describing population",
        "min": 1
      },
      {
        "id": "MeasureReport.group.population.count",
        "path": "MeasureReport.group.population.count",
        "comment": "Must include a count unless code = measureObservation",
        "constraint": [
          {
            "key": "ele-1",
            "severity": "error",
            "human": "All FHIR elements must have a @value or children",
            "expression": "hasValue() or (children().count() > id.count())",
            "xpath": "@value|f:*|h:div",
            "source": "http://hl7.org/fhir/StructureDefinition/Element"
          },
          {
            "key": "must-have-count-except-measure-observation",
            "severity": "error",
            "human": "A population must have a count when code is not measure-observation",
            "expression": "type = 'measure-observation' or count.exists()",
            "xpath": "f:type/f:code/@value = 'measure-observation' or f:count",
            "source": "http://ainq.com/fhir/us/saner/StructureDefinition/saner-bed-measure-report"
          }
        ]
      },
      {
        "id": "MeasureReport.group.population.subjectResults",
        "path": "MeasureReport.group.population.subjectResults",
        "comment": "summary reports are not subject-list reports",
        "max": "0"
      },
      {
        "id": "MeasureReport.group.measureScore",
        "path": "MeasureReport.group.measureScore",
        "comment": "Must include measureScore to simplify use by others, even if measureScore = count",
        "min": 1
      },
      {
        "id": "MeasureReport.group.stratifier",
        "path": "MeasureReport.group.stratifier",
        "slicing": {
          "discriminator": [
            {
              "type": "pattern",
              "path": "code"
            }
          ],
          "rules": "open",
          "description": "Slice based on the group.stratifier.code pattern"
        }
      },
      {
        "id": "MeasureReport.group.stratifier.code",
        "path": "MeasureReport.group.stratifier.code",
        "comment": "If there is a stratifier, it must be coded",
        "min": 1,
        "max": "1",
        "binding": {
          "strength": "extensible",
          "valueSet": "http://ainq.com/fhir/us/saner/ValueSet/Fields"
        }
      },
      {
        "id": "MeasureReport.group.stratifier.stratum",
        "path": "MeasureReport.group.stratifier.stratum",
        "slicing": {
          "discriminator": [
            {
              "type": "pattern",
              "path": "component.code"
            }
          ],
          "rules": "open",
          "description": "Slice based on the group.stratifier.stratum.code pattern"
        }
      },
      {
        "id": "MeasureReport.group.stratifier.stratum.component.code",
        "path": "MeasureReport.group.stratifier.stratum.component.code",
        "comment": "If there is a stratum, it must be coded",
        "binding": {
          "strength": "extensible",
          "valueSet": "http://ainq.com/fhir/us/saner/ValueSet/Fields"
        }
      },
      {
        "id": "MeasureReport.group.stratifier.stratum.population.code",
        "path": "MeasureReport.group.stratifier.stratum.population.code",
        "comment": "Must include code describing population",
        "min": 1
      },
      {
        "id": "MeasureReport.group.stratifier.stratum.population.count",
        "path": "MeasureReport.group.stratifier.stratum.population.count",
        "comment": "Must include a count unless code = measureObservation",
        "constraint": [
          {
            "key": "ele-1",
            "severity": "error",
            "human": "All FHIR elements must have a @value or children",
            "expression": "hasValue() or (children().count() > id.count())",
            "xpath": "@value|f:*|h:div",
            "source": "http://hl7.org/fhir/StructureDefinition/Element"
          },
          {
            "key": "must-have-count-except-measure-observation",
            "severity": "error",
            "human": "A population must have a count when code is not measure-observation",
            "expression": "type = 'measure-observation' or count.exists()",
            "xpath": "f:type/f:code/@value = 'measure-observation' or f:count",
            "source": "http://ainq.com/fhir/us/saner/StructureDefinition/saner-bed-measure-report"
          }
        ]
      },
      {
        "id": "MeasureReport.group.stratifier.stratum.population.subjectResults",
        "path": "MeasureReport.group.stratifier.stratum.population.subjectResults",
        "comment": "summary reports are not subject-list reports",
        "max": "0"
      }
    ]
  }
}
